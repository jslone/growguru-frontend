{
  "name": "growguru",
  "version": "0.0.0",
  "description": "The growguru application",
  "keywords": [
    "angularjs",
    "app"
  ],
  "main": "app://demoapp/index.html",
  "chromium-args": "-allow-file-access-from-files --disable-logging",
  "window": {
    "title": "growguru",
    "icon": "growguru.png",
    "toolbar": false,
    "width": 800,
    "height": 600,
    "position": "mouse",
    "min_width": 400,
    "min_height": 200
  },
  "webkit": {
    "page-cache": false
  },
  "devDependencies": {
    "grunt": "~0.4",
    "grunt-dom-munger": "~3.4",
    "grunt-angular-templates": "~0.5",
    "grunt-ngmin": "0.0.3",
    "grunt-contrib-connect": "~0.6",
    "grunt-contrib-copy": "~0.5",
    "grunt-contrib-clean": "~0.5",
    "grunt-contrib-coffee": "^0.10.1",
    "grunt-contrib-concat": "~0.3",
    "grunt-contrib-cssmin": "~0.7",
    "grunt-contrib-uglify": "~0.2",
    "grunt-contrib-jshint": "~0.9",
    "grunt-contrib-htmlmin": "~0.1",
    "grunt-contrib-imagemin": "~0.4",
    "grunt-contrib-less": "~0.8",
    "grunt-contrib-sass": "~0.7",
    "grunt-newer": "~0.6.1",
    "grunt-contrib-watch": "~0.6",
    "grunt-browser-output": "0.1.0",
    "load-grunt-tasks": "~0.2",
    "karma": "~0.12.6",
    "grunt-karma": "~0.8.3",
    "karma-jasmine": "~0.1.5",
    "karma-phantomjs-launcher": "~0.1.4",
    "karma-chrome-launcher": "~0.1.3",
    "karma-mocha-reporter": "~0.2.5",
    "karma-firefox-launcher": "~0.1.3",
    "time-grunt": "~0.2.1",
    "grunt-shell-spawn": "^0.3.0",
    "nodewebkit": "^0.8.6-3",
    "karma-nodewebkit-launcher": "0.0.8"
  }
}
